<h1>Listing functions</h1>

<table>
  <tr>
  </tr>
  
<% for function in @functions %>
  <tr>
    <td><%= link_to 'Show', function_path(function) %></td>
    <td><%= link_to 'Edit', edit_function_path(function) %></td>
    <td><%= link_to 'Destroy', function_path(function), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New function', new_function_path %>