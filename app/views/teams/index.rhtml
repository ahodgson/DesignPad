<h1>Listing teams</h1>

<table>
  <tr>
  </tr>
  
<% for team in @teams %>
  <tr>
    <td><%= link_to 'Show', team_path(team) %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team_path(team), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New team', new_team_path %>