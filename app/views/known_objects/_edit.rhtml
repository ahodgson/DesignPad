<% remote_form_for(:known_object, :url => known_object_path(@known_object), :html => { :method => :put }) do |form| %>
<fieldset>
  <legend>New Object</legend>
  <%= render :partial => "remote_form", :locals => { :form => form } %>
  <%= submit_tag "Update", :class => "submit" %>
  <%= button_to_function "Cancel" do |page|
        page.visual_effect :dropOut, "edit_known_object"
  end %>
</fieldset>
<% end %>
